{
  "name": "clusters",
  "version": "0.0.1",
  "description": "Data processing done on many Browsers with WebWorkers",
  "license": "GNU AGPLv3",
  "keywords": [
    "clusters",
    "Web Workers",
    "WebWorkers"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hdf/clusters.git"
  },
  "dependencies": {
    "spdy": ">= 1.32.0",
    "helmet": ">= 0.9.1",
    "express": ">= 4.13.0",
    "forever": ">= 0.14.1",
    "hogan.js": ">= 3.0.2",
    "compression": ">= 1.5.0",
    "body-parser": ">= 1.13.1",
    "basic-auth": ">= 1.0.2",
    "consolidate": ">= 0.13.1",
    "minimist": ">= 1.1.1",
    "uglify-js": ">= 2.4.23",
    "clean-css": ">= 3.3.3",
    "serve-favicon": ">= 2.3.0"
  },
  "devDependencies": {
    "mocha": ">= 2.2.5",
    "istanbul": ">= 0.3.15"
  },
  "engines": [
    "node >= 0.12.4",
    "iojs >= 2.3.0"
  ],
  "main": "server",
  "scripts": {
    "start": "forever start -w --minUptime 1000 --spinSleepTime 1000 server --port 8082 --forever",
    "stop": "forever stop server",
    "test": "mocha --reporter spec test/tests.js",
    "istanbul": "istanbul cover server -- --forever --push"
  }
}
